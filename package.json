{
  "name": "mcp-remote-oauth-client-provider",
  "version": "1.0.4",
  "description": "OAuth client provider for MultiServerMCPClient",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run build",
    "release:patch": "npm version patch --no-git-tag-version && npm publish --access public",
    "release:minor": "npm version minor --no-git-tag-version && npm publish --access public",
    "release:major": "npm version major --no-git-tag-version && npm publish --access public",
    "release:beta": "npm version prerelease --preid beta --no-git-tag-version && npm publish --tag beta --access public"
  },
  "keywords": [
    "mcp",
    "oauth",
    "client",
    "provider",
    "multiserver"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.2",
    "axios": "^1.6.5",
    "open": "^8.4.2"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
